@article{holey2022heightaveraged,
  title = {Height-{{Averaged Navier}}--{{Stokes Solver}} for {{Hydrodynamic Lubrication}}},
  author = {Holey, Hannes and Codrignani, Andrea and Gumbsch, Peter and Pastewka, Lars},
  year = {2022},
  month = mar,
  journal = {Tribology Letters},
  volume = {70},
  number = {2},
  pages = {36},
  issn = {1573-2711},
  doi = {10.1007/s11249-022-01576-5},
  urldate = {2024-02-20},
  abstract = {The cornerstone of thin-film flow modeling is the Reynolds equation---a lower-dimensional representation of the Navier--Stokes equation. The derivation of the Reynolds equation is based on explicit assumptions about the constitutive behavior of the fluid that prohibit applications in multiscale scenarios based on measured or atomistically simulated data. Here, we present a method that treats the macroscopic flow evolution and the calculation of local cross-film stresses as separate yet coupled problems---the so-called macro and micro problem. The macro problem considers mass and momentum balance for compressible fluids in a height-averaged sense and is solved using a time-explicit finite-volume scheme. Analytical solutions for the micro problem are derived for common constitutive laws and implemented into the Height-averaged Navier--Stokes (HANS) solver. We demonstrate the validity of our solver on examples, including mass-conserving cavitation, inertial effects, wall slip, and non-Newtonian fluids. The presented method is not limited to these fixed-form relations and may therefore be useful for testing constitutive relations obtained from experiment or simulation.},
  langid = {english},
  keywords = {Cavitation in hydrodynamics,Compressibility in hydrodynamics,Fluid mechanics methods,Hydrodynamic lubrication},
  annotation = {13},
  file = {/mnt/dat/Zotero/storage/R824C82F/Holey et al. - 2022 - Height-Averaged Navier–Stokes Solver for Hydrodyna.pdf}
}
